<html>
    <body>
        <h1>Loop</h1>
        <script>
            var members = ['egoing', 'duru', 'taeho'];
            document.write('<ol>');            
            var i = 0;
            While(i < 3){
                document.write('<li>'+members[i]+'<li>');
                i = i + 1;
            }
            document.write('</ol>'); 
        </script>
    </body>
</html>